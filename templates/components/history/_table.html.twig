{% if data is not null %}
<div style="height:750px;overflow-y:scroll;display:block;">
    <table class="table table-striped ">
        <thead>
        <tr>
            <th>Date</th>
            <th>Open</th>
            <th>High</th>
            <th>Low</th>
            <th>Close</th>
            <th>Volume</th>
        </tr>
        </thead>
        <tbody>
        {% for row in data.prices %}
            <tr>
                <td>{{ row.date.format('Y-m-d') }}</td>
                <td>{{ row.open|escape }}</td>
                <td>{{ row.high|escape }}</td>
                <td>{{ row.low|escape }}</td>
                <td>{{ row.close|escape }}</td>
                <td>{{ row.volume|escape }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
{% endif %}
